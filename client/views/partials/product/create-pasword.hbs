<el-form :model="createPasswordForm" 
         :rules="createPasswordValidate" 
         ref="createPasswordForm" 
         label-width="120px" 
         class="demo-ruleForm"
         :label-position="labelPositionTop">
    <el-form-item label="Chọn chức năng">
        <el-select class="w-100" v-model="createPasswordForm.btn" multiple placeholder="Chọn người được phép truy cập">
            <el-option
                v-for="item in listData.optionsBnt"
                :key="item.value"
                :label="item.label"
                :value="item.value">
            </el-option>
        </el-select>
    </el-form-item>
    <el-form-item label="Người dùng được phép truy cập">
        <el-select class="w-100" v-model="createPasswordForm.useraccess" multiple placeholder="Chọn người được phép truy cập">
            <el-option
                v-for="item in listData.optionsUser"
                :key="item.value"
                :label="item.label"
                :value="item.value">
            </el-option>
        </el-select>
    </el-form-item>
    <el-form-item label="Người dùng được phép truy cập đổi mật khẩu">
        <el-select class="w-100" v-model="createPasswordForm.usereditpassword" multiple placeholder="Chọn người được phép truy cập">
            <el-option
                v-for="item in listData.optionsUser"
                :key="item.value"
                :label="item.label"
                :value="item.value">
            </el-option>
        </el-select>
    </el-form-item>
    <el-form-item label="Mật khẩu">
        <el-input placeholder="Vui lòng nhập mật khẩu" v-model="createPasswordForm.password" show-password></el-input>
    </el-form-item>
    <el-form-item label="Xác nhận mật khẩu">
        <el-input placeholder="Vui lòng xác nhận nhập mật khẩu" v-model="createPasswordForm.respassword" show-password></el-input>
    </el-form-item>
    <el-form-item>
    <el-button type="primary" @click="createPasswordCreate('createPasswordForm')">Xác nhận</el-button>
  </el-form-item>
</el-form>