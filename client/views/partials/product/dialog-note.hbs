<el-dialog :title="title" :visible.sync="dialogFormNoteProductVisible" class="cn-note">
    <el-form :label-position="labelPositionTop" :model="noteForm" :rules="noteValidate" ref="noteForm"
        label-width="120px" class="demo-ruleForm">
        <el-form-item label="Tiêu đề ghi chú">
            <el-input v-model="noteForm.title" placeholder="Vui lòng nhập tiêu đề"></el-input>
        </el-form-item>
        <el-form-item label="Tiêu đề ghi chú">
            <el-input type="textarea" v-model="noteForm.content" placeholder="Vui lòng nhập ghi chú"></el-input>
        </el-form-item>
        <el-form-item label="Thời gian">
            <el-time-picker class="w-100" is-range v-model="noteForm.date" range-separator="To"
                start-placeholder="Start time" end-placeholder="End time">
            </el-time-picker>
        </el-form-item>
        <el-form-item label="Trạng thái">
            <el-radio-group v-model="noteForm.status">
                <el-radio-button v-for="item in listData.radioStatusNote" :label="item.label">

                </el-radio-button>
            </el-radio-group>
        </el-form-item>
        <el-form-item>
            <el-button type="primary" @click="createNoteProduct('noteForm')">Lưu thay đổi</el-button>
            <el-button>Đóng</el-button>
        </el-form-item>
    </el-form>
</el-dialog>